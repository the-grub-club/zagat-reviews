    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "css-loader": "^2.1.1",
    "csv-writer": "^1.3.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "file-loader": "^3.0.1",
    "fs": "0.0.1-security",
    "jquery": "^3.4.1",
    "newrelic": "^5.10.0",
    "path": "^0.12.7",
    "pg": "^7.11.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^2.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "style-loader": "^0.23.1",
    "url": "^0.11.0",
    "winston": "^3.2.1"
  },
